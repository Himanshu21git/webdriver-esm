{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && serenity-bdd update",
    "clean": "rimraf target",
    "test": "failsafe clean test:execute test:report",
    "test:execute": "wdio wdio.conf.ts",
    "test:report": "serenity-bdd run --features ./features",
    "start": "mkdirp target/site/serenity && npx http-server -p 8080 target/site/serenity -s -o"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "nuxt": "^3.8.2",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5",
    "@cucumber/cucumber": "^9.6.0",
    "@serenity-js/assertions": "^3.12.0",
    "@serenity-js/console-reporter": "^3.12.0",
    "@serenity-js/core": "^3.12.0",
    "@serenity-js/cucumber": "^3.12.0",
    "@serenity-js/serenity-bdd": "^3.12.0",
    "@serenity-js/web": "^3.12.0",
    "@serenity-js/webdriverio": "^3.12.0",
    "@wdio/cli": "^8.17.0",
    "@wdio/globals": "^8.17.0",
    "@wdio/local-runner": "^8.17.0",
    "@wdio/spec-reporter": "^8.17.0",
    "webdriverio": "^8.17.0",
    "http-server": "^14.1.1",
    "mkdirp": "^3.0.1",
    "npm-failsafe": "^1.0.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
